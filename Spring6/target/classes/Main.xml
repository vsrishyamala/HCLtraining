<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean name="course1" class="org.hcl.Course">
		<property name="name" value="java"></property>
		<property name="mentor" value="sri"></property>
		<property name="fee" value="1000"></property>
	</bean>
	<bean name="course2" class="org.hcl.Course">
		<property name="name" value="selenium"></property>
		<property name="mentor" value="shyamala"></property>
		<property name="fee" value="10000"></property>
	</bean>
	<bean name="course3" class="org.hcl.Course">
		<property name="name" value="python"></property>
		<property name="mentor" value="ss"></property>
		<property name="fee" value="12000"></property>
	</bean>
</beans>
